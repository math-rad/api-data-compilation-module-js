{
    "endpoint": "https://api.example.com/v3/",
    "input": [
        "image"
    ],
    "reference": [
        {
            "path": "images/$/conversion/$",
            "with": [
                "image.id",
                "image.format"
            ],
            "functionIndex": [
                "getIndex"
            ],
            "track": [
                "info"
            ],
            "tree": {
                "index.index2": [
                    "*:info.$",
                    "dateTaken:info.imageDate"
                ],
                "index2": [
                    "[abc,cba,bac]:info.&getIndex($)",
                    ""
                ]
            }
        }
    ]
}